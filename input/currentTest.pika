exec{
	var a:= "hello world".
	# a:= "".
	var x:= 3.
	var y:= 5.
	var i:= 0.
	print "a: '", a, "' with length: ", length a, _n_, _n_.
	while(i < length a){
		print i,": ", a, _n_.
		i:= i + 1.
	}
	var b:= a[x,y].
	print _n_,_n_,"substring: '", b, "' with length:"; length b, _n_, _n_.
	i :=0.
	while(i < length b){
		print i,": ", b[i], _n_.
		i:= i + 1.
	}
}